<script>
	class Student{
		Info(name,age,address){
			this.name = prompt('이름 입력 ');
			this.age = prompt('나이입력 ');
			this.address = prompt('주소 입력');
			
		}
		get name(){
			return this.name;
		}
		get age(){
			return this.age;
		}
		get address(){
			return this.address;
		}
		insert(){
			prompt('입력 ' );
		}
		printOut(){
			return'이름 : '+this.name+'<br>나이 : '+this.age+'<br>주소 : '+this.address;
		}
		
	}
	
	var stu1 = new Student();
	
			
