<script>
	var lotto=[];
	var num = 0;
	var show =[];
	
	for(var i = 1 ; i<=45;i++){
		lotto.push(i);
	}
	
		
		
	for(var j = 0 ; j<6; j++){
		num = Math.floor(Math.random()*lotto.length);	
		show[j] = lotto[num];
	}
	console.log(show.toString());
		
	
</script>
