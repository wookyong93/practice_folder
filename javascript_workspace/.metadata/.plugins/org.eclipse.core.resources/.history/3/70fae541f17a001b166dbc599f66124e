<script>
	class Student{
		name;
		age;
		address;
		set setName(name){
			this.name = name;
		}
		set setAge(age){
			this.age = age;
		}
		set setAddress(address){
			this.address = address;
		}
		outPut(){
			return '이름 : '+this.name + '나이 : '+this.age + '주소 : '+this.address+'<br>';
		}
		
	}

	var stu1 = new Studnet();
	var stu2 = new Studnet();
	var stu3 = new Student();
	stu1.setName(prompt('이름 입력 '));
	stu1.setAge(prompt('나이 입력'));
	stu1.setAddress(prompt('주소입력'));
	document.wrtie(stu1.outPut());
