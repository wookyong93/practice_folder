<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>문제9</title>
<script>
	class Student{
		name;
		age;
		address;
		set setName(name){
			this.name = name;
		}
		set setAge(age){
			this.age = age;
		}
		set setAddress(address){
			this.address = address;
		}
		output(){
			return '이름 : '+this.name + '나이 : '+this.age + '주소 : '+this.address+'<br>';
		}
		
	}

	var stu1 = new Student();
	var stu2 = new Student();
	var stu3 = new Student();
	stu1.name = prompt('이름 입력 ');
	stu1.age = prompt('나이 입력 ');
	stu1.address=prompt('주소 입력 ');
	document.write(stu1.output());
</script>
</head>
