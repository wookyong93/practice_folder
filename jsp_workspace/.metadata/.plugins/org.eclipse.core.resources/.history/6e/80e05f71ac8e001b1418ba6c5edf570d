<script type="text/javascript">
	/* $(documnent) = > document의 DOM(Document Object Mdoel)이 
	로드되는 이벤트 처리 함수입니다.*/
	$(document).ready(function(){
		alert($("#unique2").html());
		/* $("#unique2").html => html에서 해당 아이디를 호출한다. */
	});
</script>
</head>
